{
  "name": "ireporter",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "lint": "eslint .",
    "fix": "eslint --fix .",
    "test": "nyc mocha --require babel-polyfill --require babel-register ./test/ --timeout 10000 --exit",
    "coverage": "nyc npm test && nyc report --reporter=text-lcov | coveralls",
    "clean": "rm -rf build && mkdir build",
    "build-babel": "babel -d ./build ./app -s",
    "build": "npm run clean && npm run build-babel",
    "start": "npm run build && node ./build/server.js"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "chai": "^4.2.0",
    "debug": "^4.1.0",
    "express": "^4.16.4",
    "http-errors": "^1.7.1",
    "less-middleware": "^3.0.1",
    "morgan": "^1.9.1",
    "validator": "^10.9.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-istanbul": "^5.1.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "chai-http": "^4.2.0",
    "coveralls": "^3.0.2",
    "cryptiles": "^4.1.3",
    "eslint": "^5.9.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "hoek": "^6.1.1",
    "is-valid-coordinates": "^1.0.0",
    "istanbul": "^0.4.5",
    "mocha": "^5.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^13.1.0"
  }
}
